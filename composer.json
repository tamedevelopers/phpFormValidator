{
    "name": "peterson/validator",
    "type": "library",
    "description": "PHP Form Validator is a small no-depencies library for PHP codes.",
    "keywords": ["php form validator", "form validator", "form", "php forms", "php", "forms", "easy validation"],
    "homepage": "https://github.com/tamedevelopers/validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Fredrick Peterson", 
            "email": "fredi.peterson2000@gmail.com"
        },
        {
            "name": "Tame Developers", 
            "email": "tamedevelopers@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "UltimateValidator\\": "src/"
        },
        "classmap": ["src/*"]
    },
    "autoload-dev": {
        "psr-4": {
            "UltimateValidator\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
