{
    "name": "peterson/ultimate-validator",
    "type": "library",
    "description": "PHP Form Validator is a small no-depencies library for php form validation.",
    "keywords": ["php-form-validator", "form-validator", "form", "php-form", "php", "forms", "easy validation", "easy-form"],
    "homepage": "https://github.com/tamedevelopers/UltimateValidator",
    "license": "MIT",
    "authors": [
        {"name": "Fredrick Peterson", "email": "fredi.peterson2000@gmail.com"},
        {"name": "Tame Developers", "email": "tamedevelopers@gmail.com"}
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "UltimateValidator\\": "src/"
        },
        "classmap": ["src/*"]
    },
    "autoload-dev": {
        "psr-4": {
            "UltimateValidator\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
